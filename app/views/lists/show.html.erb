<h1><%= @list.title %></h1>
<%= link_to "Edit List", edit_list_path(@list) %>

<%= link_to "New Task", new_list_task_path(@list) %>
<% if @tasks.any? %>
  <% @tasks.each do |task| %>
    <p><%= task.body %><%= link_to "edit", edit_list_task_path(@list, task) %></p>
  <% end %>
<% else %>
  <p>No tasks</p>
<% end %>

<%= link_to "back to project", @project %>

